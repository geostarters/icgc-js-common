{
  "name": "@geostarters/common",
  "homepage": "https://github.com/geostarters/icgc-js-common",
  "bugs": "https://github.com/geostarters/icgc-js-common/issues",
  "version": "0.1.13",
  "description": "ICGC JS common",
  "author": "@geostarters",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/geostarters/icgc-js-common.git"
  },
  "engines": {
    "node": ">=6.4.0"
  },
  "devDependencies": {
    "@geostarters/eslint-config": "0.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "8.0.0",
    "brfs": "^1.4.4",
    "browserify": "^15.2.0",
    "bubleify": "^1.1.0",
    "coveralls": "^3.0.0",
    "derequire": "^2.0.6",
    "documentation": "^5.3.5",
    "envify": "^4.1.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-node": "^5.2.1",
    "esm": "^3.0.84",
    "execcommand-copy": "^1.1.0",
    "flow-bin": "^0.64.0",
    "flow-coverage-report": "^0.4.1",
    "flow-remove-types": "^1.2.3",
    "github-slugger": "^1.2.0",
    "glob": "^7.1.2",
    "in-publish": "^2.0.0",
    "minifyify": "^7.3.5",
    "npm-run-all": "^4.1.2",
    "opn": "^5.3.0",
    "package-json-versionify": "^1.0.4",
    "present": "^1.0.0",
    "prismjs": "^1.11.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "st": "^1.2.2",
    "tap": "^11.1.0",
    "unassertify": "^2.1.0",
    "unflowify": "^1.0.1",
    "watchify": "^3.10.0"
  },
  "browserify": {
    "transform": [
      "unflowify",
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-0"
          ]
        }
      ],
      [
        "bubleify",
        {
          "bubleError": true,
          "transforms": {
            "dangerousForOf": true
          },
          "objectAssign": "Object.assign"
        }
      ],
      "package-json-versionify",
      "unassertify",
      "brfs"
    ]
  },
  "scripts": {
    "build-dev": "browserify src/index.js --debug --ignore-transform unassertify --standalone ICGCCommon > dist/ICGCCommon.js",
    "watch-dev": "watchify src/index.js --debug --ignore-transform unassertify --standalone ICGCCommon --outfile dist/ICGCCommon-dev.js --verbose",
    "build-min": "browserify src/index.js --debug --plugin [minifyify --map ICGCCommon.js.map --output dist/ICGCCommon.js.map] --standalone ICGCCommon | derequire > dist/ICGCCommon.js",
    "build-react": "run-s build-unflow",
    "build-web": "run-s lint build-min",
    "build-all": "run-s build-web build-react",
    "build-unflow": "flow-remove-types  src/ --out-dir dist/",
    "start-server": "st --no-cache -H 127.0.0.1 --port 9966 --index index.html .",
    "start": "run-p watch-dev start-server open",
    "open": "node build/dev-server.js",
    "build-docs": "documentation build --github --format json --config ./docs/documentation.yml --output docs/components/api.json src/index.js",
    "lint": "eslint --fix --cache --ignore-path .gitignore src test",
    "lint-docs": "documentation lint src/index.js",
    "test": "run-s lint test-unit",
    "test-unit": "tap --reporter classic --no-coverage test/unit",
    "test-flow": "flow .",
    "test-cov": "nyc --require=flow-remove-types/register --reporter=text-summary --reporter=lcov --cache npm run test-unit",
    "pre-production": "run-s lint build-min test test-cov"
  },
  "files": [
    "build/",
    "dist/",
    "flow-typed/",
    "src/",
    ".flowconfig"
  ],
  "dependencies": {
    "axios": "^0.18.0"
  },
  "peerDependencies": {
    "mapbox-gl": "^0.51.0"
  }
}
